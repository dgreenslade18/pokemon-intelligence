# RapidAPI Key - Required for Pokemon TCG API and eBay Sold Items API
# Get your key from https://rapidapi.com/
RAPID_API_KEY=your_rapidapi_key_here

# Application Configuration  
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app

# Optional: For development
NODE_ENV=development 

# eBay API Configuration
# Get these from https://developer.ebay.com/my/keys
EBAY_APP_ID=your_ebay_app_id_here
EBAY_ACCESS_TOKEN=your_ebay_access_token_here

# Optional: For enhanced features
EBAY_CERT_ID=your_ebay_cert_id_here
EBAY_DEV_ID=your_ebay_dev_id_here 